<h2>My Messages</h2>
<table border="1">
  <tr>
    <th>Recipient</th>
    <th>Title</th>
    <th>Status</th>
    <th>Created At</th>
  </tr>
  <% messages.forEach(msg => { %>
    <tr>
      <td><%= msg.recipient %></td>
      <td><%= msg.title %></td>
      <td><%= msg.status %></td>
      <td><%= msg.created_at %></td>
    </tr>
  <% }) %>
</table>
